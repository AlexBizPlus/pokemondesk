{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": true,
    "jsx": "react",
    "sourceMap": true,
    "outDir": "./public"
  },
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Recommended"
}
